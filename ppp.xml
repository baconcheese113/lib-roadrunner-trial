<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sbml.org/sbml/level2/version4
https://sbml.org/documents/specifications/level-2/version-4/release-1/sbml.level-2.version-4.release-1.xsd" level="2" version="4">
  <model id="PPP" name="Pentose Phosphate Pathway">
    <listOfCompartments>
      <compartment id="cytosol" size="1" name="Cytosol"/>
    </listOfCompartments>
    <listOfSpecies>
      <!-- Reused species from your list with identical definitions -->
      <species id="G6P" name="Glucose 6 Phosphate" compartment="cytosol" initialConcentration="2.45"/>
      <species id="F6P" name="Fructose 6 Phosphate" compartment="cytosol" initialConcentration="0.62"/>
      <species id="TRIO" name="Triose-phosphate" compartment="cytosol" initialConcentration="0.96"/>
      <species id="CO2" name="CO2" compartment="cytosol" initialConcentration="1" boundaryCondition="true"/>
      <species id="H" name="Proton (cytosolic)" compartment="cytosol" initialConcentration="1" boundaryCondition="true" constant="true"/>
      <!-- New species for PPP -->
      <species id="P6GL" name="6-Phosphogluconolactone" compartment="cytosol" initialConcentration="0"/>
      <species id="P6G" name="6-Phosphogluconate" compartment="cytosol" initialConcentration="0"/>
      <species id="Ru5P" name="Ribulose-5-phosphate" compartment="cytosol" initialConcentration="0"/>
      <species id="R5P" name="Ribose-5-phosphate" compartment="cytosol" initialConcentration="0"/>
      <species id="X5P" name="Xylulose-5-phosphate" compartment="cytosol" initialConcentration="0"/>
      <species id="S7P" name="Sedoheptulose-7-phosphate" compartment="cytosol" initialConcentration="0"/>
      <species id="E4P" name="Erythrose-4-phosphate" compartment="cytosol" initialConcentration="0"/>
      <species id="NADP" name="Nicotinamide Adenine Dinucleotide Phosphate (oxidized)" compartment="cytosol" initialConcentration="0.1"/>
      <species id="NADPH" name="Nicotinamide Adenine Dinucleotide Phosphate (reduced)" compartment="cytosol" initialConcentration="0.1"/>
    </listOfSpecies>
    <listOfReactions>
      <!-- Oxidative Phase -->
      <reaction id="vG6PDH" name="Glucose-6-phosphate Dehydrogenase" reversible="false">
        <listOfReactants>
          <speciesReference species="G6P"/>
          <speciesReference species="NADP"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P6GL"/>
          <speciesReference species="NADPH"/>
          <speciesReference species="H"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>VmG6PDH</ci>
              <apply>
                <divide/>
                <ci>G6P</ci>
                <apply>
                  <plus/>
                  <ci>KmG6PDH_G6P</ci>
                  <ci>G6P</ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci>NADP</ci>
                <apply>
                  <plus/>
                  <ci>KmG6PDH_NADP</ci>
                  <ci>NADP</ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmG6PDH" value="10" units="mM_per_min"/>
            <parameter id="KmG6PDH_G6P" value="0.1" units="mM"/>
            <parameter id="KmG6PDH_NADP" value="0.01" units="mM"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction id="v6PGLase" name="6-Phosphogluconolactonase" reversible="false">
        <listOfReactants>
          <speciesReference species="P6GL"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P6G"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>k6PGLase</ci>
              <ci>P6GL</ci>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="k6PGLase" value="100" units="per_min"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction id="v6PGDH" name="6-Phosphogluconate Dehydrogenase" reversible="false">
        <listOfReactants>
          <speciesReference species="P6G"/>
          <speciesReference species="NADP"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Ru5P"/>
          <speciesReference species="NADPH"/>
          <speciesReference species="CO2"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>Vm6PGDH</ci>
              <apply>
                <divide/>
                <ci>P6G</ci>
                <apply>
                  <plus/>
                  <ci>Km6PGDH_P6G</ci>
                  <ci>P6G</ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci>NADP</ci>
                <apply>
                  <plus/>
                  <ci>Km6PGDH_NADP</ci>
                  <ci>NADP</ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="Vm6PGDH" value="10" units="mM_per_min"/>
            <parameter id="Km6PGDH_P6G" value="0.1" units="mM"/>
            <parameter id="Km6PGDH_NADP" value="0.01" units="mM"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <!-- Non-Oxidative Phase -->
      <reaction id="vRPI" name="Ribose-5-phosphate Isomerase" reversible="true">
        <listOfReactants>
          <speciesReference species="Ru5P"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="R5P"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci>kRPI_f</ci>
                <ci>Ru5P</ci>
              </apply>
              <apply>
                <times/>
                <ci>kRPI_r</ci>
                <ci>R5P</ci>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="kRPI_f" value="50" units="per_min"/>
            <parameter id="kRPI_r" value="50" units="per_min"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction id="vRPE" name="Ribulose-5-phosphate Epimerase" reversible="true">
        <listOfReactants>
          <speciesReference species="Ru5P"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="X5P"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci>kRPE_f</ci>
                <ci>Ru5P</ci>
              </apply>
              <apply>
                <times/>
                <ci>kRPE_r</ci>
                <ci>X5P</ci>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="kRPE_f" value="50" units="per_min"/>
            <parameter id="kRPE_r" value="50" units="per_min"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction id="vTK1" name="Transketolase 1" reversible="false">
        <listOfReactants>
          <speciesReference species="R5P"/>
          <speciesReference species="X5P"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S7P"/>
          <speciesReference species="TRIO"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>VmTK1</ci>
              <apply>
                <divide/>
                <ci>R5P</ci>
                <apply>
                  <plus/>
                  <ci>KmTK1_R5P</ci>
                  <ci>R5P</ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci>X5P</ci>
                <apply>
                  <plus/>
                  <ci>KmTK1_X5P</ci>
                  <ci>X5P</ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmTK1" value="20" units="mM_per_min"/>
            <parameter id="KmTK1_R5P" value="0.1" units="mM"/>
            <parameter id="KmTK1_X5P" value="0.1" units="mM"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction id="vTA" name="Transaldolase" reversible="false">
        <listOfReactants>
          <speciesReference species="S7P"/>
          <speciesReference species="TRIO"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="E4P"/>
          <speciesReference species="F6P"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>VmTA</ci>
              <apply>
                <divide/>
                <ci>S7P</ci>
                <apply>
                  <plus/>
                  <ci>KmTA_S7P</ci>
                  <ci>S7P</ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci>TRIO</ci>
                <apply>
                  <plus/>
                  <ci>KmTA_TRIO</ci>
                  <ci>TRIO</ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmTA" value="20" units="mM_per_min"/>
            <parameter id="KmTA_S7P" value="0.1" units="mM"/>
            <parameter id="KmTA_TRIO" value="0.1" units="mM"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      <reaction id="vTK2" name="Transketolase 2" reversible="false">
        <listOfReactants>
          <speciesReference species="E4P"/>
          <speciesReference species="X5P"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="F6P"/>
          <speciesReference species="TRIO"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>VmTK2</ci>
              <apply>
                <divide/>
                <ci>E4P</ci>
                <apply>
                  <plus/>
                  <ci>KmTK2_E4P</ci>
                  <ci>E4P</ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci>X5P</ci>
                <apply>
                  <plus/>
                  <ci>KmTK2_X5P</ci>
                  <ci>X5P</ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmTK2" value="20" units="mM_per_min"/>
            <parameter id="KmTK2_E4P" value="0.1" units="mM"/>
            <parameter id="KmTK2_X5P" value="0.1" units="mM"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>