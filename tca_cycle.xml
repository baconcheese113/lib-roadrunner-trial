<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sbml.org/sbml/level2/version4
https://sbml.org/documents/specifications/level-2/version-4/release-1/sbml.level-2.version-4.release-1.xsd" level="2" version="4">
  <model id="TCA_Cycle">
    <!-- Compartments (consistent with oxphos.xml) -->
    <listOfCompartments>
      <compartment id="mitochondrion" size="1" name="Mitochondrial Matrix"/>
    </listOfCompartments>
    
    <!-- Species for TCA cycle in the mitochondrion -->
    <listOfSpecies>
      <species id="PYR_m" compartment="mitochondrion" initialConcentration="0.1" name="Pyruvate"/>
      <species id="AcCoA_m" compartment="mitochondrion" initialConcentration="0.05" name="Acetyl-CoA"/>
      <species id="OAA_m" compartment="mitochondrion" initialConcentration="0.01" name="Oxaloacetate"/>
      <species id="Cit_m" compartment="mitochondrion" initialConcentration="0.5" name="Citrate"/>
      <species id="IsoCit_m" compartment="mitochondrion" initialConcentration="0.05" name="Isocitrate"/>
      <species id="aKG_m" compartment="mitochondrion" initialConcentration="0.1" name="Alpha-Ketoglutarate"/>
      <species id="SuccCoA_m" compartment="mitochondrion" initialConcentration="0.01" name="Succinyl-CoA"/>
      <species id="Succ_m" compartment="mitochondrion" initialConcentration="0.1" name="Succinate"/>
      <species id="Fum_m" compartment="mitochondrion" initialConcentration="0.05" name="Fumarate"/>
      <species id="Mal_m" compartment="mitochondrion" initialConcentration="0.1" name="Malate"/>
      <species id="CoA_m" compartment="mitochondrion" initialConcentration="0.5" name="Coenzyme A"/>
      <species id="FAD_m" compartment="mitochondrion" initialConcentration="0.5" name="Flavin Adenine Dinucleotide (oxidized)"/>
      <species id="FADH2_m" compartment="mitochondrion" initialConcentration="0.05" name="Flavin Adenine Dinucleotide (reduced)"/>
      <species id="CO2_m" compartment="mitochondrion" initialConcentration="1" boundaryCondition="true" constant="true" name="Carbon Dioxide"/>
      <species id="iP_m" compartment="mitochondrion" initialConcentration="2.44" name="Inorganic Phosphate"/>
    </listOfSpecies>
    
    <!-- Reactions for TCA cycle -->
    <listOfReactions>
      <!-- Pyruvate transport from cytosol (glycolysis.xml) to mitochondrion -->
      <reaction id="vPYR_trans" reversible="true" name="Pyruvate Transport">
        <listOfReactants>
          <speciesReference species="PYR"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="PYR_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kPYR_trans </ci>
              <apply>
                <minus/>
                <ci> PYR </ci>
                <ci> PYR_m </ci>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="kPYR_trans" value="10" units="per_min" name="Pyruvate Transport Rate Constant"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Pyruvate dehydrogenase (PDH) -->
      <reaction id="vPDH" reversible="false" name="Pyruvate Dehydrogenase">
        <listOfReactants>
          <speciesReference species="PYR_m"/>
          <speciesReference species="CoA_m"/>
          <speciesReference species="NAD_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="AcCoA_m"/>
          <speciesReference species="CO2_m"/>
          <speciesReference species="NADH_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> VmPDH </ci>
                <ci> PYR_m </ci>
                <ci> CoA_m </ci>
                <ci> NAD_m </ci>
              </apply>
              <apply>
                <times/>
                <apply>
                  <plus/>
                  <ci> KmPDH_PYR </ci>
                  <ci> PYR_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmPDH_CoA </ci>
                  <ci> CoA_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmPDH_NAD </ci>
                  <ci> NAD_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmPDH" value="100" units="mM_per_min" name="Maximum Velocity of PDH"/>
            <parameter id="KmPDH_PYR" value="0.1" units="mM" name="Michaelis Constant for Pyruvate"/>
            <parameter id="KmPDH_CoA" value="0.01" units="mM" name="Michaelis Constant for CoA"/>
            <parameter id="KmPDH_NAD" value="0.1" units="mM" name="Michaelis Constant for NAD"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Citrate synthase (CS) -->
      <reaction id="vCS" reversible="false" name="Citrate Synthase">
        <listOfReactants>
          <speciesReference species="AcCoA_m"/>
          <speciesReference species="OAA_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Cit_m"/>
          <speciesReference species="CoA_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> VmCS </ci>
                <ci> AcCoA_m </ci>
                <ci> OAA_m </ci>
              </apply>
              <apply>
                <times/>
                <apply>
                  <plus/>
                  <ci> KmCS_AcCoA </ci>
                  <ci> AcCoA_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmCS_OAA </ci>
                  <ci> OAA_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmCS" value="50" units="mM_per_min" name="Maximum Velocity of CS"/>
            <parameter id="KmCS_AcCoA" value="0.01" units="mM" name="Michaelis Constant for Acetyl-CoA"/>
            <parameter id="KmCS_OAA" value="0.005" units="mM" name="Michaelis Constant for Oxaloacetate"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Aconitase (Acon) -->
      <reaction id="vAcon" reversible="true" name="Aconitase">
        <listOfReactants>
          <speciesReference species="Cit_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="IsoCit_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kAcon_f </ci>
              <apply>
                <minus/>
                <ci> Cit_m </ci>
                <apply>
                  <divide/>
                  <ci> IsoCit_m </ci>
                  <ci> KeqAcon </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="kAcon_f" value="100" units="per_min" name="Forward Rate Constant of Aconitase"/>
            <parameter id="KeqAcon" value="1" units="dimensionless" name="Equilibrium Constant of Aconitase"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Isocitrate dehydrogenase (IDH) -->
      <reaction id="vIDH" reversible="false" name="Isocitrate Dehydrogenase">
        <listOfReactants>
          <speciesReference species="IsoCit_m"/>
          <speciesReference species="NAD_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="aKG_m"/>
          <speciesReference species="CO2_m"/>
          <speciesReference species="NADH_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> VmIDH </ci>
                <ci> IsoCit_m </ci>
                <ci> NAD_m </ci>
              </apply>
              <apply>
                <times/>
                <apply>
                  <plus/>
                  <ci> KmIDH_IsoCit </ci>
                  <ci> IsoCit_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmIDH_NAD </ci>
                  <ci> NAD_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmIDH" value="50" units="mM_per_min" name="Maximum Velocity of IDH"/>
            <parameter id="KmIDH_IsoCit" value="0.1" units="mM" name="Michaelis Constant for Isocitrate"/>
            <parameter id="KmIDH_NAD" value="0.1" units="mM" name="Michaelis Constant for NAD"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Î±-Ketoglutarate dehydrogenase (AKGDH) -->
      <reaction id="vAKGDH" reversible="false" name="Alpha-Ketoglutarate Dehydrogenase">
        <listOfReactants>
          <speciesReference species="aKG_m"/>
          <speciesReference species="CoA_m"/>
          <speciesReference species="NAD_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="SuccCoA_m"/>
          <speciesReference species="CO2_m"/>
          <speciesReference species="NADH_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> VmAKGDH </ci>
                <ci> aKG_m </ci>
                <ci> CoA_m </ci>
                <ci> NAD_m </ci>
              </apply>
              <apply>
                <times/>
                <apply>
                  <plus/>
                  <ci> KmAKGDH_aKG </ci>
                  <ci> aKG_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmAKGDH_CoA </ci>
                  <ci> CoA_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmAKGDH_NAD </ci>
                  <ci> NAD_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmAKGDH" value="50" units="mM_per_min" name="Maximum Velocity of AKGDH"/>
            <parameter id="KmAKGDH_aKG" value="0.1" units="mM" name="Michaelis Constant for Alpha-Ketoglutarate"/>
            <parameter id="KmAKGDH_CoA" value="0.01" units="mM" name="Michaelis Constant for CoA"/>
            <parameter id="KmAKGDH_NAD" value="0.1" units="mM" name="Michaelis Constant for NAD"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Succinyl-CoA synthetase (SCS) -->
      <reaction id="vSCS" reversible="false" name="Succinyl-CoA Synthetase">
        <listOfReactants>
          <speciesReference species="SuccCoA_m"/>
          <speciesReference species="ADP_m"/>
          <speciesReference species="iP_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Succ_m"/>
          <speciesReference species="ATP_m"/>
          <speciesReference species="CoA_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> VmSCS </ci>
                <ci> SuccCoA_m </ci>
                <ci> ADP_m </ci>
                <ci> iP_m </ci>
              </apply>
              <apply>
                <times/>
                <apply>
                  <plus/>
                  <ci> KmSCS_SuccCoA </ci>
                  <ci> SuccCoA_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmSCS_ADP </ci>
                  <ci> ADP_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmSCS_iP </ci>
                  <ci> iP_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmSCS" value="50" units="mM_per_min" name="Maximum Velocity of SCS"/>
            <parameter id="KmSCS_SuccCoA" value="0.01" units="mM" name="Michaelis Constant for Succinyl-CoA"/>
            <parameter id="KmSCS_ADP" value="0.1" units="mM" name="Michaelis Constant for ADP"/>
            <parameter id="KmSCS_iP" value="1" units="mM" name="Michaelis Constant for Inorganic Phosphate"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Succinate dehydrogenase (SDH) -->
      <reaction id="vSDH" reversible="false" name="Succinate Dehydrogenase">
        <listOfReactants>
          <speciesReference species="Succ_m"/>
          <speciesReference species="FAD_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Fum_m"/>
          <speciesReference species="FADH2_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> VmSDH </ci>
                <ci> Succ_m </ci>
                <ci> FAD_m </ci>
              </apply>
              <apply>
                <times/>
                <apply>
                  <plus/>
                  <ci> KmSDH_Succ </ci>
                  <ci> Succ_m </ci>
                </apply>
                <apply>
                  <plus/>
                  <ci> KmSDH_FAD </ci>
                  <ci> FAD_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="VmSDH" value="50" units="mM_per_min" name="Maximum Velocity of SDH"/>
            <parameter id="KmSDH_Succ" value="0.5" units="mM" name="Michaelis Constant for Succinate"/>
            <parameter id="KmSDH_FAD" value="0.01" units="mM" name="Michaelis Constant for FAD"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Fumarase (FUM) -->
      <reaction id="vFUM" reversible="true" name="Fumarase">
        <listOfReactants>
          <speciesReference species="Fum_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Mal_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kFUM_f </ci>
              <apply>
                <minus/>
                <ci> Fum_m </ci>
                <apply>
                  <divide/>
                  <ci> Mal_m </ci>
                  <ci> KeqFUM </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="kFUM_f" value="100" units="per_min" name="Forward Rate Constant of Fumarase"/>
            <parameter id="KeqFUM" value="4" units="dimensionless" name="Equilibrium Constant of Fumarase"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- Malate dehydrogenase (MDH) -->
      <reaction id="vMDH" reversible="true" name="Malate Dehydrogenase">
        <listOfReactants>
          <speciesReference species="Mal_m"/>
          <speciesReference species="NAD_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="OAA_m"/>
          <speciesReference species="NADH_m"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci> kMDH_f </ci>
                <ci> Mal_m </ci>
                <ci> NAD_m </ci>
              </apply>
              <apply>
                <times/>
                <ci> kMDH_r </ci>
                <ci> OAA_m </ci>
                <ci> NADH_m </ci>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="kMDH_f" value="100" units="per_mM_per_min" name="Forward Rate Constant of MDH"/>
            <parameter id="kMDH_r" value="10" units="per_mM_per_min" name="Reverse Rate Constant of MDH"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <!-- FADH2 oxidation (feeds into OxPhos ETC) -->
      <reaction id="vETC_FADH2" reversible="false" name="FADH2 Oxidation">
        <listOfReactants>
          <speciesReference species="FADH2_m" stoichiometry="1"/>
          <speciesReference species="O2" stoichiometry="0.5"/>
          <speciesReference species="H" stoichiometry="3"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="FAD_m" stoichiometry="1"/>
          <speciesReference species="H2O" stoichiometry="1"/>
          <speciesReference species="He" stoichiometry="6"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kETC_FADH2 </ci>
              <ci> FADH2_m </ci>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="kETC_FADH2" value="60" units="per_min" name="Rate Constant of FADH2 Oxidation"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>