<?xml version="1.0" encoding="UTF-8"?>
<sbml xmlns="http://www.sbml.org/sbml/level2/version4" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.sbml.org/sbml/level2/version4
https://sbml.org/documents/specifications/level-2/version-4/release-1/sbml.level-2.version-4.release-1.xsd" level="2" version="4">
  <model id="OxPhos">
    <!-- Compartments (reused from cleaned_model.xml) -->
    <listOfCompartments>
      <compartment id="cytosol" size="1" name="Cytosol"/>
      <compartment id="mitochondrion" name="Mitochondrial Matrix" size="1"/>
    </listOfCompartments>
    
    <!-- Species (new mitochondrial species and reused existing ones) -->
    <listOfSpecies>
      <!-- New mitochondrial species for OxPhos -->
      <species id="NAD_m" compartment="mitochondrion" initialConcentration="1.2" name="Nicotinamide Adenine Dinucleotide (oxidized)"/>
      <species id="NADH_m" compartment="mitochondrion" initialConcentration="0.39" name="Nicotinamide Adenine Dinucleotide (reduced)"/>
      <species id="ATP_m" compartment="mitochondrion" initialConcentration="1.5" name="Adenosine Triphosphate (mitochondrial)"/>
      <species id="ADP_m" compartment="mitochondrion" initialConcentration="2.1" name="Adenosine Diphosphate (mitochondrial)"/>
      
      <!-- Existing species from cleaned_model.xml reused -->
      <species id="ATP" compartment="cytosol" initialConcentration="3" name="Adenosine Triphosphate (cytosolic)"/>
      <species id="ADP" compartment="cytosol" initialConcentration="0.7" name="Adenosine Diphosphate (cytosolic)"/>
      <species id="O2_m" compartment="mitochondrion" initialConcentration="1" boundaryCondition="true" constant="true" name="Oxygen"/>
      <species id="H_m" compartment="mitochondrion" initialConcentration="1" boundaryCondition="true" constant="true" name="Proton"/>
      <species id="H" compartment="cytosol" initialConcentration="1" boundaryCondition="true" constant="true" name="Proton (cytosolic)"/>
      <species id="H2O_m" compartment="mitochondrion" initialConcentration="1" boundaryCondition="true" constant="true" name="Water"/>
      <species id="P" compartment="cytosol" initialConcentration="5" name="High energy phosphates" />
      <species id="iP_m" compartment="mitochondrion" initialConcentration="5" name="Inorganic Phosphate"/>
      <species id="ROS_m" compartment="mitochondrion" initialConcentration="0.001"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="kETC" value="30" units="per_min" name="Electron Transport Chain Rate Constant (NADH)"/>
      <parameter id="Km_NADH_m" value="0.1" units="mM" name="Michaelis Constant for NADH_m in ETC"/>
      <parameter id="Km_O2_m" value="0.1" units="mM" name="Michaelis Constant for O2_m in ETC"/>
      <parameter id="kETC_FADH2" value="30" units="per_min" name="Electron Transport Chain Rate Constant (FADH2)"/>
      <parameter id="Km_FADH2_m" value="0.1" units="mM" name="Michaelis Constant for FADH2_m in ETC"/>
      <parameter id="Km_O2_FADH2" value="0.1" units="mM" name="Michaelis Constant for O2_m with FADH2"/>
    </listOfParameters>
    
    <!-- Reactions for OxPhos -->
    <listOfReactions>
      <!-- Electron Transport Chain (ETC) -->
      <reaction id="vETC" reversible="false" name="Electron Transport Chain (NADH)">
        <listOfReactants>
          <speciesReference species="NADH_m" stoichiometry="1"/>
          <speciesReference species="O2_m" stoichiometry="0.5"/>
          <speciesReference species="H_m" stoichiometry="5"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="NAD_m" stoichiometry="1"/>
          <speciesReference species="H2O_m" stoichiometry="1"/>
          <speciesReference species="H" stoichiometry="10"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>kETC</ci>
              <apply>
                <divide/>
                <ci>NADH_m</ci>
                <apply><plus/><ci>Km_NADH_m</ci><ci>NADH_m</ci></apply>
              </apply>
              <apply>
                <divide/>
                <ci>O2_m</ci>
                <apply><plus/><ci>Km_O2_m</ci><ci>O2_m</ci></apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>


      <reaction id="vETC_FADH2" reversible="false" name="Electron Transport Chain (FADH2)">
        <listOfReactants>
          <speciesReference species="FADH2_m" stoichiometry="1"/>
          <speciesReference species="O2_m" stoichiometry="0.5"/>
          <speciesReference species="H_m" stoichiometry="5"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="FAD_m" stoichiometry="1"/>
          <speciesReference species="H2O_m" stoichiometry="1"/>
          <speciesReference species="H" stoichiometry="10"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci>kETC_FADH2</ci>
              <apply>
                <divide/>
                <ci>FADH2_m</ci>
                <apply><plus/><ci>Km_FADH2_m</ci><ci>FADH2_m</ci></apply>
              </apply>
              <apply>
                <divide/>
                <ci>O2_m</ci>
                <apply><plus/><ci>Km_O2_FADH2</ci><ci>O2_m</ci></apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      
      <!-- ATP Synthase (Chemiosmosis) -->
      <reaction id="vATPsyn" name="ATP Synthase" reversible="false">
        <listOfReactants>
          <speciesReference species="ADP_m" stoichiometry="1"/>
          <speciesReference species="iP_m" stoichiometry="1"/>
          <speciesReference species="H_m" stoichiometry="3"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="ATP_m" stoichiometry="1"/>
          <speciesReference species="H2O_m" stoichiometry="1"/>
          <speciesReference species="H" stoichiometry="3"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> Vmax_ATPsyn </ci>
              <apply>
                <divide/>
                <ci> ADP_m </ci>
                <apply>
                  <plus/>
                  <ci> Km_ADP_m </ci>
                  <ci> ADP_m </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> iP_m </ci>
                <apply>
                  <plus/>
                  <ci> Km_Pi_m </ci>
                  <ci> iP_m </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> H_m </ci>
                <apply>
                  <plus/>
                  <ci> Km_H_m </ci>
                  <ci> H_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="Vmax_ATPsyn" value="12" units="mM_per_min" name="Maximum rate of ATP synthase"/>
            <parameter id="Km_ADP_m" value="0.03" units="mM" name="Michaelis constant for mitochondrial ADP"/>
            <parameter id="Km_Pi_m" value="0.2" units="mM" name="Michaelis constant for mitochondrial Pi"/>
            <parameter id="Km_H_m" value="0.1" units="mM" name="Michaelis constant for proton concentration"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>

      <!-- Pi/H+ Symport realistic model -->
      <reaction id="vPiH_symport" name="Phosphate-H+ Symport" reversible="false">
        <listOfReactants>
          <speciesReference species="P" stoichiometry="1"/>
          <speciesReference species="H" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="iP_m" stoichiometry="1"/>
          <speciesReference species="H_m" stoichiometry="1"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> Vmax_PiH </ci>
              <apply>
                <divide/>
                <ci> P </ci>
                <apply>
                  <plus/>
                  <ci> Km_PiH </ci>
                  <ci> P </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> H </ci>
                <apply>
                  <plus/>
                  <ci> Km_H_PiH </ci>
                  <ci> H </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> Km_iP_m </ci>
                <apply>
                  <plus/>
                  <ci> Km_iP_m </ci>
                  <ci> iP_m </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="Vmax_PiH" value="20" units="mM_per_min" name="Max transport rate"/>
            <parameter id="Km_PiH" value="0.5" units="mM" name="Km for cytosolic P"/>
            <parameter id="Km_H_PiH" value="0.1" units="mM" name="Km for proton concentration"/>
            <parameter id="Km_iP_m" value="10.0" units="mM" name="Inhibition by mitochondrial iP_m"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      <reaction id="vPi_import" name="Cytosolic Pi Import" reversible="false">
        <listOfReactants>
          <speciesReference species="iP_m"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="P"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> Vm_Pi_import </ci>
              <apply>
                <divide/>
                <ci> iP_m </ci>
                <apply>
                  <plus/>
                  <ci> Km_Pi_import </ci>
                  <ci> iP_m </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> Km_P </ci>
                <apply>
                  <plus/>
                  <ci> Km_P </ci>
                  <ci> P </ci>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="Vm_Pi_import" value="1" units="mMpermin"/>
            <parameter id="Km_Pi_import" value="0.1" units="mM"/>
            <parameter id="Km_P" value="1.0" units="mM"/>
          </listOfParameters>
        </kineticLaw>
        
      </reaction>
      
      <!-- Adenine Nucleotide Translocator (ANT) -->
      <reaction id="vANT" reversible="true" name="Adenine Nucleotide Translocator">
        <listOfReactants>
          <speciesReference species="ATP_m" stoichiometry="1"/>
          <speciesReference species="ADP" stoichiometry="1"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="ATP" stoichiometry="1"/>
          <speciesReference species="ADP_m" stoichiometry="1"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> Vmax_ANT </ci>
              <apply>
                <minus/>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> ATP_m </ci>
                    <ci> ADP </ci>
                  </apply>
                  <apply>
                    <plus/>
                    <ci> Km_ATP_m </ci>
                    <ci> ATP_m </ci>
                    <ci> Km_ADP </ci>
                    <ci> ADP </ci>
                  </apply>
                </apply>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> ATP </ci>
                    <ci> ADP_m </ci>
                  </apply>
                  <apply>
                    <plus/>
                    <ci> Km_ATP </ci>
                    <ci> ATP </ci>
                    <ci> Km_ADP_m </ci>
                    <ci> ADP_m </ci>
                  </apply>
                </apply>
              </apply>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="Vmax_ANT" value="5.0" units="mM_per_min" name="Maximum ANT transport rate"/>
            <parameter id="Km_ATP_m" value="0.05" units="mM" name="Km for mitochondrial ATP"/>
            <parameter id="Km_ADP" value="0.05" units="mM" name="Km for cytosolic ADP"/>
            <parameter id="Km_ATP" value="0.05" units="mM" name="Km for cytosolic ATP"/>
            <parameter id="Km_ADP_m" value="0.05" units="mM" name="Km for mitochondrial ADP"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>

      <reaction id="vROS" name="ROS Production" reversible="false">
        <listOfProducts>
          <speciesReference species="ROS_m" stoichiometry="1"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> leakFactor </ci>
              <ci> kETC </ci>
              <apply>
                <divide/>
                <ci> NADH_m </ci>
                <apply>
                  <plus/>
                  <ci> Km_NADH_m </ci>
                  <ci> NADH_m </ci>
                </apply>
              </apply>
              <apply>
                <divide/>
                <ci> Km_O2_m </ci> <!-- Inverted O2 dependence -->
                <apply>
                  <plus/>
                  <ci> Km_O2_m </ci>
                  <ci> O2_m </ci>
                </apply>
              </apply>
            </apply>            
          </math>
          <listOfParameters>
            <parameter id="leakFactor" value="0.01" units="dimensionless"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>

      <reaction id="vAntioxidants" name="ROS Scavenging" reversible="false">
        <listOfReactants>
          <speciesReference species="ROS_m" stoichiometry="1"/>
        </listOfReactants>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> ROS_clearance </ci>
              <ci> ROS_m </ci>
            </apply>
          </math>
          <listOfParameters>
            <parameter id="ROS_clearance" value="0.1" units="permin"/>
          </listOfParameters>
        </kineticLaw>
      </reaction>
      
      
    </listOfReactions>
  </model>
</sbml>